<VSTemplate Version="2.0.0" Type="Project" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005">
  <TemplateData>
    <Name>Dynamic library</Name>
    <Description>A project for creating a dynamic library</Description>
    <Icon>__TemplateIcon.ico</Icon>
    <ProjectType>D</ProjectType>
    <SortOrder>50</SortOrder>
    <NumberOfParentCategoriesToRollUp>1</NumberOfParentCategoriesToRollUp>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>DynamicLibrary</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
  </TemplateData>
  <TemplateContent>
    <Project File="DynamicLib.visualdproj" ReplaceParameters="true">
      <ProjectItem ReplaceParameters="true" OpenInEditor="true">dllmain.d</ProjectItem>
      <ProjectItem ReplaceParameters="true" OpenInEditor="false">dll.def</ProjectItem>
    </Project>
  </TemplateContent>
</VSTemplate>
